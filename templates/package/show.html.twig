{% extends 'base.html.twig' %}

{% block title %}Package{% endblock %}

{% block body %}
    <h1>Package</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ package.id }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ package.quantity }}</td>
            </tr>
            <tr>
                <th>QuantityDrug</th>
